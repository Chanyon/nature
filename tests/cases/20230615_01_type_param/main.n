import 'mod.n'

mod.nullable<[i8]> foo = null
println(foo)

foo = [1 as i8, 2, 3]
let foo as [i8]
print(foo[0], foo[1], foo[2], '\n')

// struct box
var b = mod.box<i8, i16> {
    width = 5,
    length = 10
}

println('i8 area=', @unsafe_load(b).area())
println('i8 area=', @unsafe_load(b).width * @unsafe_load(b).length as i8)

var car = mod.box<f32, f32> {
    width = 3.1,
    length = 6.1
}
println('float area=', @unsafe_load(car).area())
println('float area=', @unsafe_load(car).width * @unsafe_load(car).length)
