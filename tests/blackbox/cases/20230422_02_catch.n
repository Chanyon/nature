fn hello() {
    println('hello nature')
    throw 'world error'
}

var err = try hello()
if err.has {
    print('catch err: ', err.msg, '\n')
}

fn one(bool need):int {
    if need {
        throw 'i am one error'
    }
    return 2
}

var (s, err) = try one(true)
if err.has {
    print('one(true) has err:', err.msg, '\n')
} else {
    print('one(true) not err, s=', s, '\n')
}

var (s1, err) = try one(false)
if err.has {
    print('one(false) has err:', err.msg, '\n')
} else {
    print('one(false) not err, s1=', s1, '\n')
}