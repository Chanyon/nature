fn hello() {
    println('hello nature')
    throw 'hello error'
}

hello() catch err {
    println('catch err:', err.msg)
    continue
    continue
    println('hello world 1')
}

fn world(bool e):int {
    if e {
        throw 'world error'
    }
    return 233
}

int foo = world(true) catch err {
    println('catch err:', err.msg)
    continue 12
}
println('foo =', foo)

foo = world(false) catch err {
    println('catch err:', err.msg)
    continue 12
}
println('foo =', foo)


