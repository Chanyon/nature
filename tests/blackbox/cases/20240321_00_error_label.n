fn boom():int {
    fn test() {
        println('in test')
    }

    fn test2() {
        println('in test2')
    }

    test()

    if 1 > 2 {
        return 12
    }

    for int i = 0; i < 10; i+=1 {
        if i % 2 == 0 {
            test()
        } else {
            test2()
        }

        if i == 9 {
            break
        }
    }

    test2()

    return 2333
}

boom()