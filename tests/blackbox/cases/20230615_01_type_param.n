// struct box
type box<t0, t1> = struct {
	t0 width
	t1 length
	var area = fn(self s):t0 {
	    return s.width * s.length as t0
	}
}

var b = box<i8, i16> {
    width = 5,
    length = 10
}

println(b.area())

var car = box<f32, f32> {
    width = 5.1,
    length = 10.1
}
