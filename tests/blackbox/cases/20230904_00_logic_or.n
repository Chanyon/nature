import std
import std.util
import time

// 启用快速强制 gc
runtime_set_force_gc()

fn logf(string fmt, ...[any] args) {
    var msg = std.sprintf(fmt, ...args)
    var t = time.now()
    std.printf('log: %v time: %v\n', msg, t.datetime())
}


logf('hello33 %v', 'world33', '333')
logf('hello55 %v, %v', 'world55', 'haha55', '622', '777', '888', '999', '1000', '11', '12')
