import coroutine as co
import time

fn boom() {
    for int i = 0; i <= 5; i+=1 {
        co.sleep(1000)
    }
    println('sub co done')
}

println('co create start', time.now().timestamp())
for int i = 0; i < 100000; i+=1 {
    go boom()
}
println('co create end', time.now().timestamp())

co.sleep(10 * 1000)

println('main co done', time.now().timestamp())
