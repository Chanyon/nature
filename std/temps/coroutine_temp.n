type rt_coroutine_t = struct{
    bool main
    bool solo
    i32 status
    cptr aco
    cptr fn
    cptr p
    // cptr args
    cptr result
}

fn coroutine_create(fn() f, int flags):ptr<rt_coroutine_t>

fn coroutine_run(fn() f, int flags):ptr<rt_coroutine_t>

fn coroutine_yield()

fn coroutine_sleep(int ms)

fn coroutine_get():ptr<rt_coroutine_t>