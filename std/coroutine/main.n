import coroutine_temp

var SOLO = 1 << 1

type coroutine_t = struct{
    i32 status
}

fn create(fn() f, int flags):coroutine_t {
    var rt_co = coroutine_create(f, flags)

    return coroutine_t{
        status = rt_co.status
    }
}

fn run(fn() f, int flags):coroutine_t {
    var rt_co = coroutine_async(f, flags)

    return coroutine_t{
        status = rt_co.status
    }
}

fn sleep(int ms) {
    coroutine_sleep(ms)
}

fn yield() {
    coroutine_yield()
}
